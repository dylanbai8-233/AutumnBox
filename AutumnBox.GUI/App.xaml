<Application x:Class="AutumnBox.GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Windows/AppLoadingWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="AutumnBox.Font">Microsoft YaHei Light</FontFamily>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/I18N/Langs/zh-CN.xaml"/>
                <ResourceDictionary Source="/Resources/Themes/DreamTheme.xaml"/>
                <ResourceDictionary Source="/Resources/Urls.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
