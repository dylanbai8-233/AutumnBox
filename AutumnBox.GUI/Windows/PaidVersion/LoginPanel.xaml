<fp:FastPanelChild
    xmlns:fp="clr-namespace:AutumnBox.GUI.UI.Fp"
    x:Class="AutumnBox.GUI.Windows.PaidVersion.LoginPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutumnBox.GUI.Windows.PaidVersion"
             mc:Ignorable="d" 
            Height="288" Width="419.5">
    <Grid x:Name="GridBase" Background="{DynamicResource FastPanelBackgroundBrushKey}">
        <TextBox x:Name="InputAccount" HorizontalAlignment="Left" Height="24" Margin="121,144,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="175"/>
        <PasswordBox x:Name="InputPassword" HorizontalAlignment="Left" Margin="121,173,0,0" VerticalAlignment="Top" Width="175"/>
        <Button x:Name="BtnLogin" Content="Button" Click="BtnLogin_Click" HorizontalAlignment="Left" Margin="121,212,0,0" VerticalAlignment="Top" Width="175"/>

    </Grid>
</fp:FastPanelChild>
