<fp:FastPanelChild
    xmlns:fp="clr-namespace:AutumnBox.GUI.UI.Fp"
    x:Class="AutumnBox.GUI.PaidVersion.AccountPanel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:local="clr-namespace:AutumnBox.GUI.PaidVersion"
    mc:Ignorable="d" 
    d:DesignHeight="537" d:DesignWidth="940">
    <Grid Background="{DynamicResource FastPanelBackgroundBrushKey}">
        <TextBlock Foreground="{DynamicResource ForegroundOnPrimaryBrushKey}" Margin="10,68,10,0" TextAlignment="Center" TextWrapping="Wrap" Text="{DynamicResource msgDonatedVersionThanks}" Style="{DynamicResource AtmbxTextBlockStyle}" FontSize="30" VerticalAlignment="Top"/>
        <TextBlock TextAlignment="Center" x:Name="TBAccountInfo" Style="{DynamicResource AtmbxTextBlockStyle}" Foreground="{DynamicResource ForegroundOnPrimaryBrushKey}" FontSize="20" Margin="10,159,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="204"/>
        <Button FontSize="20" Click="Button_Click" Foreground="{DynamicResource ForegroundOnPrimaryBrushKey}" Style="{DynamicResource AtmbxButtonStyle}" Content="{DynamicResource btnManageAccount}" Margin="278,368,277,0" VerticalAlignment="Top"/>

    </Grid>
