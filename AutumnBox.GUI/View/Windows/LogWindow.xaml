<Window x:Class="AutumnBox.GUI.View.Windows.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutumnBox.GUI.View.Windows"
        xmlns:viewmodel="clr-namespace:AutumnBox.GUI.ViewModel"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="LogWindow" Height="525.5" Width="791.378"
        Closed="Window_Closed"
        >
    <DockPanel>
        <StackPanel Margin="5"  DockPanel.Dock="Bottom" Orientation="Horizontal">
            <Button Margin="5" HorizontalAlignment="Right" VerticalAlignment="Bottom" Content="{DynamicResource btnCopy}" Click="Button_Click_1"/>
            <Button Margin="5"  HorizontalAlignment="Right" VerticalAlignment="Bottom" Content="Go to the end" Click="Button_Click"/>
        </StackPanel>
        <TextBox DockPanel.Dock="Top" x:Name="TextBox" Style="{x:Null}" IsReadOnly="True" TextWrapping="Wrap"/>
    </DockPanel>
</Window>
