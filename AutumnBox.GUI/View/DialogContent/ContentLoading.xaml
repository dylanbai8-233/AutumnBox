<UserControl x:Class="AutumnBox.GUI.View.DialogContent.ContentLoading"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             FontFamily="{DynamicResource AutumnBox.Font}"
             mc:Ignorable="d" Height="65.14" Width="209.239">
    <Grid>

        <ProgressBar
          Style="{DynamicResource MaterialDesignCircularProgressBar}"
          IsIndeterminate="True"
          Value="0" Height="Auto" Width="20" Margin="10,22,0,22" HorizontalAlignment="Left" />

        <TextBlock Text="{DynamicResource msgDoing}" Margin="35,22,10,0" Height="33" VerticalAlignment="Top"></TextBlock>
    </Grid>
</UserControl>
