<controls:AtmbDialogContent
    xmlns:controls="clr-namespace:AutumnBox.GUI.View.Controls"
    x:Class="AutumnBox.GUI.View.DialogContent.ContentDisconnectDevice"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutumnBox.GUI.View.DialogContent"
             mc:Ignorable="d" Height="130.435" Width="292.976">
    <Grid>
        <StackPanel>
            <Button Command="{Binding DisconnectAndDisable}" Style="{DynamicResource MaterialDesignRaisedButton}" Margin="5" Content="{DynamicResource ContentDisconnectNetDeviceBtnDisableAndDisconnect}"/>
            <Button Command="{Binding Disconnect}" Style="{DynamicResource MaterialDesignFlatButton}" Margin="5"  Content="{DynamicResource ContentDisconnectNetDeviceBtnOnlyDisconnect}"/>
            <Button Command="materialDesign:DialogHost.CloseDialogCommand" Style="{DynamicResource MaterialDesignFlatButton}" Margin="5" Content="{DynamicResource ContentDisconnectNetDeviceBtnCancel}"/>
        </StackPanel>
    </Grid>
</controls:AtmbDialogContent>
