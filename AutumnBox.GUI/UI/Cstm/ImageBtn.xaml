<UserControl x:Class="AutumnBox.GUI.UI.Cstm.ImageBtn"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutumnBox.GUI.UI.Cstm"
             mc:Ignorable="d" 
             x:Name="m"
             d:DesignHeight="50" d:DesignWidth="50" MouseEnter="UserControl_MouseEnter" MouseLeave="UserControl_MouseLeave">
    <Grid>
        <Image RenderOptions.BitmapScalingMode="Fant" x:Name="MainImg" Source="{Binding Path=Image,ElementName=m}"/>
    </Grid>
</UserControl>
