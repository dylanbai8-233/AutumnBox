<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ThemeBase.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    <system:String x:Key="ThemeName">For love-Pink</system:String>
    <Color x:Key="SecondaryColor">#ffebee</Color>
    <Color x:Key="PrimaryLightColor">#fb7299</Color>
    <Color x:Key="PrimaryMidColor">#fb7299</Color>
    <Color x:Key="PrimaryDarkColor">#fb7299</Color>
</ResourceDictionary>